-static int8_t drecrypt_process_emm(struct s_reader *rdr, uint32_t provId, uint8_t *emm, uint32_t *keysAdded)
-{
-	uint32_t i, keyIdent;
-	uint16_t keyName, emmLen, emmDataLen;
