-	if ((namespace & 0xA0000000) != 0xA0000000) return 0;   // Value isn't flagged as namespace
-	if (namespace == 0xA0000000) return 0;                  // Empty namespace
-	if (orbital > 3599) return 0;                           // Allow only DVB-S
