-			}
-		}
-		else // Date-coded keys are disabled, don't evaluate expiration date
-		{
-			if (printMsg == 1 || printMsg == 2) cs_log("Key found: F %08X %s", provider, keyExpDate);
-			return 1;
-		}
-	}
