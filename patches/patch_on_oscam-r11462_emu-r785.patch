-			keyidx = 0x60;
-			keyclass = 0x05;
-			key1offset = 0x62;
-			key2offset = 0x8B;
-			break;
-
-		default:
-			return 1; // Not supported
-	}
-
-	switch (provId & 0xFF)
-	{
-		case 0x11:
-		{
-			snprintf(curKeyName, EMU_MAX_CHAR_KEYNAME, "3B%02X", emm[keyclass]);
