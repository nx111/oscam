-		data = malloc(aligned_len);
-		if(data == NULL)
-			return 0;
-		memset(data, 0xFF, aligned_len);
-		memcpy(data, in, len);
-	}
-	int i,offset;
-	for(i = 0,offset = 0; i < (aligned_len / BLOCK_SIZE) && offset < maxlen; i++, offset += BLOCK_SIZE)
-		block_encrypt(ctx, data, i * BLOCK_SIZE, out, i * BLOCK_SIZE);
-	if(data != in)
-		free(data);
-	return offset;
-}
