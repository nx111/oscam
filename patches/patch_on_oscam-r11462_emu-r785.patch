-KeyDataContainer *emu_get_key_container(char identifier)
-{
-	switch (identifier)
-	{
-		case 'W':
-			return &CwKeys;
-		case 'V':
-			return &ViKeys;
-		case 'N':
-			return &NagraKeys;
-		case 'I':
-			return &IrdetoKeys;
-		case 'S':
-			return &NDSKeys;
-		case 'F':
-			return &BissKeys;
