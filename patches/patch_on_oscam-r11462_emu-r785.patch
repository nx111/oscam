-	if (memcmp(&emm[key2offset], emm[keyidx] == 0x3B ? curECMkey56 : curECMkey3B, 32) != 0)
-	{
-		memcpy(emm[keyidx] == 0x3B ? curECMkey56 : curECMkey3B, &emm[key2offset], 32);
