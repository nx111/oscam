-	int i,offset;
-	for(i = 0,offset = 0; i < (aligned_len / BLOCK_SIZE) && offset < maxlen; i++, offset += BLOCK_SIZE)
-		block_decrypt(ctx, data, i * BLOCK_SIZE, out, i * BLOCK_SIZE);
-	return offset;
