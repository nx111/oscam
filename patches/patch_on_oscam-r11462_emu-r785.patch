-
-	if (!get_emm_key(emmKey, keyIdent, keyName, 1))
-	{
-		return 2;
-	}
-
-	// Key #1
-	for (i = 0; i < 4; i++)
-	{
-		drecrypt_decrypt(&emm[key1offset + (i * 8)], emmKey);
-	}
-
-	// Key #2
-	for (i = 0; i < 4; i++)
-	{
-		drecrypt_decrypt(&emm[key2offset + (i * 8)], emmKey);
-	}
-
-	// Key #1
-	keyName = emm[keyidx] << 8 | emm[keyclass];
