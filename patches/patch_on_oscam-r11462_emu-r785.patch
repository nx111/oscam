-
-	if (maxLength < 2)
-	{
-		(*nanoLength) = 0;
-		return EMU_NOT_SUPPORTED;
-	}
-
-	sectionLength = ((data[pos] << 8) | data[pos + 1]) & 0x0FFF;
-
-	if (pos + 2 + sectionLength > maxLength)
-	{
-		(*nanoLength) = pos;
-		return EMU_CORRUPT_DATA;
-	}
-
-	ret = parse_emm_nano_tags(data + pos + 2, sectionLength, keyIndex, keysAdded);
-
-	pos += 2 + sectionLength;
