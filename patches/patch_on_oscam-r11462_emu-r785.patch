-	// Emm type 0x86 only
-	switch (emm[4])
-	{
-		case 0x02:
-			keynum = 0x2C;
-			keyidx = 0x30;
-			keyclass = 0x26;
-			key1offset = 0x35;
-			key2offset = 0x6D;
