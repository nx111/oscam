-		emu_set_key('D', keyIdent, newKeyName, &emm[key1offset], 32, 0, NULL, NULL);
-		SAFE_MUTEX_UNLOCK(&emu_key_data_mutex);
-		(*keysAdded)++;
-
-		cs_hexdump(0, &emm[key1offset], 32, keyValue, sizeof(keyValue));
