-			snprintf(curKeyName, EMU_MAX_CHAR_KEYNAME, "56%02X", emm[keyclass]);
-			emu_find_key('D', 0x4AE114, 0, curKeyName, curECMkey56, 32, 0, 0, 0, NULL);
